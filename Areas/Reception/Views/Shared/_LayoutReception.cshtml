    <!-- Script để đảm bảo toast hoạt động -->
    <script>
        $(document).ready(function () {
            // Kết nối SignalR
            const connection = new signalR.HubConnectionBuilder()
                .withUrl("/notificationHub")
                .configureLogging(signalR.LogLevel.Information)
                .build();

            let notificationCount = 0;

            // Nhận thông báo từ server
            connection.on("ReceiveReservationNotification", function (message) {
                notificationCount++;
                $("#notificationCount").text(notificationCount);
                $("#noNotifications").hide();

                // Thêm vào danh sách thông báo (dropdown)
                const notificationItem = `
                        <div class="list-group-item list-group-item-action">
                            <div class="d-flex">
                                <div class="flex-shrink-0">
                                    <i class="fas fa-bell text-primary"></i>
                                </div>
                                <div class="flex-grow-1 ms-2">
                                    <p class="text-body mb-1">${message}</p>
                                    <span class="text-muted">${new Date().toLocaleTimeString()}</span>
                                </div>
                            </div>
                        </div>`;
                $("#notificationList").prepend(notificationItem);
                $("#notificationList").scrollTop(0);

                // Tạo và hiển thị toast thông báo
                const toastId = `newOrderToast_${Date.now()}`; // ID duy nhất cho mỗi toast
                const toastHtml = `
                        <div id="${toastId}" class="toast fade" role="alert" aria-live="assertive" aria-atomic="true" data-bs-autohide="true" data-bs-delay="5000">
                            <div class="toast-header bg-success text-white shadow-sm">
                                <i class="fas fa-bell me-2"></i>
                                <strong class="me-auto">Thông báo</strong>
                                <button type="button" class="btn-close btn-close-white" data-bs-dismiss="toast" aria-label="Close"></button>
                            </div>
                            <div class="toast-body bg-light text-dark">
                                Bạn có đơn hàng mới: ${message}
                            </div>
                        </div>`;

                // Thêm toast vào toast container
                $(".toast-container").append(toastHtml);

                // Khởi tạo và hiển thị toast
                const toastElement = document.getElementById(toastId);
                if (toastElement) {
                    const toast = new bootstrap.Toast(toastElement);
                    toast.show();
                }
            });

            // Khởi động kết nối SignalR
            connection.start()
                .then(function () {
                    console.log("SignalR connected.");
                })
                .catch(function (err) {
                    console.error("SignalR connection error: ", err.toString());
                });

            // Xóa tất cả thông báo trong dropdown
            $("#clearNotifications").click(function () {
                $("#notificationList").empty();
                $("#notificationCount").text("0");
                notificationCount = 0;
                $("#noNotifications").show();
            });

            // Khởi tạo Toast từ TempData
            const toastElements = {
                successToast: document.getElementById('successToast'),
                errorToast: document.getElementById('errorToast'),
                warningToast: document.getElementById('warningToast'),
                infoToast: document.getElementById('infoToast')
            };

            Object.keys(toastElements).forEach(function (key) {
                if (toastElements[key]) {
                    const toast = new bootstrap.Toast(toastElements[key], {
                        autohide: true,
                        delay: 5000
                    });
                    toast.show();
                }
            });
        });
    </script>
